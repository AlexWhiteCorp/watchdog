{
  "name": "vue-watchdog",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "electron:serve": "vue-cli-service electron:serve",
    "electron:build": "vue-cli-service electron:build",
    "electron:build:mac-arm": "vue-cli-service electron:build --mac --arm64 --publish=never",
    "electron:build:mac64": "vue-cli-service electron:build --mac --x64 --publish=never",
    "electron:build:win64": "vue-cli-service electron:build --win --x64 --publish=never",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test": "jest --coverage"
  },
  "main": "background.js",
  "dependencies": {
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "electron-log": "^4.4.8",
    "electron-squirrel-startup": "^1.0.0",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-flow": "^7.18.6",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-jest": "^28.1.3",
    "electron": "^19.0.8",
    "electron-builder": "^23.1.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^28.1.3",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest/globals": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "plugins": [
      "jest"
    ],
    "rules": {}
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
